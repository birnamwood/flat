<div class="session">

<h4>※管理者ログインフォーム</h4>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="t_field">
    <%= f.email_field :email, autofocus: true,  :size=>"55", placeholder: "メールアドレス", autocomplete: "email" %>
  </div><br />

  <div class="t_field">
    <%= f.password_field :password, autocomplete: "current-password", :size=>"55", placeholder: "パスワード" %>
  </div><br />


  <div class="t_actions">
    <%= f.submit "ログイン", class: "btn" %>
  </div><br />
<% end %>

</div>


