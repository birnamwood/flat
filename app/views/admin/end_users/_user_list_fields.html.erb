 <table>
  <thead>
    <tr>
      <td>アイコン</td>
      <td>ユーザー名</td>
      <td>在住</td>
      <td>投稿数</td>
      <td>登録日</td>
      <td>詳細</td>
      <td>編集</td>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
          <% if user.icon_image_id.present? %>
        <td><%= image_tag(user.icon_image_id.to_s, size: "50x50", class:"user-icon") %></td>
          <% else %>
        <td><%= image_tag("no_image.png", size: "50x50", class: "user-icon") %></td>
          <% end %>
          <% if user.nickname.blank? %>
        <td><%= link_to user.name, admin_end_user_path(user) %></td>
          <% else %>
        <td><%= link_to user.nickname, admin_end_user_path(user) %></td>
          <% end %>
        <td><%= user.prefecture.prefecture_name %></td>
        <td><%= user.posts.count %></td>
        <td><%= user.created_at.strftime("%Y年%m月%d日") %></td>
        <td><%= link_to "詳細", admin_end_user_path(user), class: "btn btn-flat" %></td>
        <td><%= link_to "編集", edit_admin_end_user_path(user), class: "btn" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate users, class: "paginate" %>